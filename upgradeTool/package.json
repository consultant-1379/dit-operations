{
  "name": "upgrade_tool",
  "private": true,
  "nyc": {
    "check-coverage": true,
    "per-file": false,
    "lines": 0,
    "functions": 0,
    "statements": 0,
    "branches": 0,
    "include": [
      "**/modules/**/*.js"
    ],
    "exclude": [
      "**/modules/**/*.tests.js",
      "**/modules/**/client/**/*.js"
    ],
    "reporter": [
      "json",
      "json-summary",
      "lcov",
      "text",
      "text-summary"
    ],
    "cache": true,
    "all": true
  },
  "scripts": {
    "lint:server": "NODE_ENV=test webpack --config webpack-server.config.js --progress",
    "lint:client": "NODE_ENV=test webpack --config webpack-client.config.js --progress",
    "test:server": "gulp test:server",
    "test:client": "gulp test:client",
    "develop:server": "NODE_ENV=development webpack --config webpack-server.config.js --progress",
    "develop:client": "NODE_ENV=development webpack --config webpack-client.config.js --progress",
    "build:server": "NODE_ENV=production webpack --config webpack-server.config.js",
    "build:client": "NODE_ENV=production webpack --config webpack-client.config.js"
  },
